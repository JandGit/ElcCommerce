<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.tkengine.elccommerce">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".UI.MainActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UI.DisplayActivity" />
        <activity android:name=".UI.UserLoginActivity" />
        <activity android:name=".UI.PayActivity" />
        <activity android:name=".UI.SearchActivity" />
        <activity android:name=".UI.CommentsActivity" />
        <activity android:name=".UI.StoreDetailsActivity" />
        <activity android:name=".UI.OrderActivity" />
        <activity android:name=".UI.PersonalinfoActivity" />
        <activity android:name=".UI.MywalletActivity" />
        <activity android:name=".UI.SignupActivity" />
        <activity android:name=".UI.CategoryActivity"></activity>
        <activity android:name=".UI.AddressActivity"/>
        <activity android:name=".UI.NewAddressActivity" />
        <activity android:name=".UI.ProvinceInfoActivity"/>
        <activity android:name=".UI.CityInfoActivity"/>
        <activity android:name=".UI.CountyInfoActivity"/>

    </application>

</manifest>